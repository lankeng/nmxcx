page{
  background-color: #f4f4f4;
}
.myclaim {
  width: 100%;
  background-color: #f4f4f4;
  padding-top: 40rpx;
  padding-bottom: 80rpx;
}

.rank-tab {
  width: 702rpx;
  margin: auto;
  background-color: #fff;
}

.rtab-th {
  width: 100%;
  height: 64rpx;
  background-color: #00c587;
  display: block;
  line-height: 64rpx;
  color: #fff;
  font-size: 30rpx;
}

.rth-rank {
  display: inline-block;
  width: 33.3%;
  text-align: center;
}

.rth-author {
  display: inline-block;
  width: 33.3%;
  text-align: center;
}

.rth-index {
  display: inline-block;
  width: 33.3%;
  text-align: center;
}

.rtab-tr {
  width: 100%;
  padding-top: 40rpx;
  padding-bottom: 30rpx;
  display: block;
  font-size: 30rpx;
  border-bottom: 1rpx solid #e6f0f3;
}

.tr-num {
  width: 33.3%;
  color: #666666;
  display: inline-block;
  text-align: center;
  font-size: 24rpx;
}

.tr-author {
  width: 33.3%;
  display: inline-block;
  text-align: center;
  font-size: 24rpx;
}

.tr-index {
  width: 33.3%;
  display: inline-block;
  color: #666666;
  text-align: center;
}

.tauthor-name {
  display: inline-block;
  color: #666666;
  font-size: 24rpx;
}
